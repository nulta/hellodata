{
    "name": "hellodata",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "test": "./node_modules/.bin/mocha -r ts-node/register -r tsconfig-paths/register -r test/helpers/hooks.ts ./test/*.ts ./test/**/*.ts ",
        "build": "npm run build-client && npm run build-server",
        "clear-dist": "rm -rf dist/* && rm -rf client/public/build/*",
        "start": "node -r module-alias/register dist/app.js",
        "dev-server": "npx nodemon --exec \"ts-node -r tsconfig-paths/register src/app.ts\" --watch src -e ts",
        "dev-client": "cd client && rollup -c -w",
        "build-client": "cd client && rollup -c",
        "build-server": "tsc"
    },
    "author": "HelloWorld makers",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "module-alias": "^2.2.2",
        "mongoose": "^6.1.1",
        "nanoid": "^3.2.0",
        "passport": "^0.5.2",
        "passport-local": "^1.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "@tsconfig/svelte": "^2.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jquery": "^3.5.10",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.7",
        "@types/passport": "^1.0.7",
        "@types/passport-local": "^1.0.34",
        "mocha": "^9.1.3",
        "mongodb-memory-server": "^8.0.4",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.5",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0",
        "svelte-check": "^2.0.0",
        "svelte-preprocess": "^4.0.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.11.0",
        "tslib": "^2.0.0",
        "typescript": "^4.4.4"
    },
    "_moduleAliases": {
